{
  "name": "pines-portfolio",
  "version": "1.0.0",
  "description": "Modern portfolio website for PineFruitDev - Full-Stack Developer & Game Development Expert",
  "author": "PineFruitDev (Sky Tomlinson)",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
