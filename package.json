    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^2.4.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
