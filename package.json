    "game-development",
    "discord-bots"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev:start": "next dev",
    "dev": "npm-run-all build dev:start",
    "test:start": "next start",
    "test": "npm-run-all build:production test:start",
    "type-check": "tsc --noEmit",
    "build": "npm-run-all clean build:compile build:next",
    "build:compile": "tsc",
    "build:next": "next build",
    "build:production": "npm-run-all build:base export",
    "clean": "npm-run-all clean:port clean:next",
    "clean:port": "npx kill-port 3000",
    "clean:next": "rimraf .next out",
    "export": "cross-env NEXT_OUTPUT=export next build"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
